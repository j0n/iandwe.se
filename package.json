{
  "name": "commune-flower",
  "version": "1.0.0",
  "description": "decoration art",
  "main": "index.js",
  "scripts": {
    "build:css": "cssjoin app/styles/main.css | autoprefixer -b 'last 2 versions' | cssmin > dist/main.css",
    "build:html": "catw 'app/index.html' -o dist/index.html -v",
    "serve": "http-server -p 9999 dist/",
    "watch:js": "watchify app/scripts/main.js -o dist/main.js -v --debug",
    "watch:css": "nodemon --watch app/styles/ -e css -x \"npm run build:css\"",
    "watch": "npm run serve & npm run watch:js & npm run build:html & npm run watch:css"
  },
  "keywords": [
    "art",
    "commune"
  ],
  "author": "Jon Andersson",
  "license": "ISC",
  "dependencies": {
    "socket.io-client": "^1.3.5",
    "watchify": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^5.1.0",
    "catw": "^0.2.0",
    "cssjoin": "^0.3.1",
    "cssmin": "^0.4.3",
    "http-server": "^0.8.0",
    "nodemon": "^1.3.7"
  }
}
